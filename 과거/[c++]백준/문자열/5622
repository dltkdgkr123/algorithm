#include <iostream>
#include <cstring>
using namespace std;

int main() {
	char c[16];
	int asc_i = 0;
	cin >> c;

	for (int i = 0; i < strlen(c); i++)
	{
		switch (c[i])
		{
		case 'A': case'B': case'C':
			asc_i += 3; break;
		case'D': case'E': case'F':
			asc_i += 4; break;
		case'G': case'H': case'I':
			asc_i += 5; break;
		case'J': case'K': case'L':
			asc_i += 6; break;
		case'M': case'N': case'O':
			asc_i += 7; break;
		case'P': case'Q': case'R': case'S':
			asc_i += 8; break;
		case'T': case'U': case'V':
			asc_i += 9; break;
		case'W': case'X': case'Y': case'Z':
			asc_i += 10; break;
		}
 	}
	cout << asc_i;
}
