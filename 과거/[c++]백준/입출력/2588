#include <iostream>
#include <string>
using namespace std;

int main() {

	int a = 0;
	int b = 0;

	cin >> a;
	cin >> b;

	int b_10 = (b % 10) * a;
	int b_100 = (((b % 100) * a) - b_10);
	int b_1000 = (b * a) - (b_10 + b_100);
		
		
	cout << b_10 << "\n";
	cout << b_100 / 10 << "\n";
	cout << b_1000 / 100 << "\n";
	cout << a * b;
}
